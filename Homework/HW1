public class HW1 {

	public static void main(String[] args) {
		
		StringBuilder sb = new StringBuilder();
		for (int i = 0; i < 1111; i++) {
			char rChar = (char)(int)(Math.random() * 94 + 33);
			sb.append(rChar);
			if (i % 100 == 0 & i != 0) {
				System.out.println();
			}
			System.out.print(rChar); 
		}

	}

}
